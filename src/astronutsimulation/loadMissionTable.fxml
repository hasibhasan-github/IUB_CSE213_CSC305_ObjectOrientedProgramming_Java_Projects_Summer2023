<AnchorPane id="AnchorPane" prefHeight="685.0" prefWidth="899.0" style="-fx-background-color: DEEPSKYBLUE;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="astronutsimulation.LoadMissionTableController">
   <children>
      <TableView fx:id="tableView" layoutX="21.0" layoutY="74.0" prefHeight="274.0" prefWidth="851.0">
        <columns>
          <TableColumn fx:id="missionNameColoumn" prefWidth="102.0" text="Mission Name" />
            <TableColumn fx:id="teamSizeColoumn" prefWidth="112.0" text="Team Size" />
          <TableColumn fx:id="missionMemberListColoumn" prefWidth="376.0" text="Mission Members" />
            <TableColumn fx:id="leaderIdColoumn" prefWidth="137.0" text="Leader ID" />
            <TableColumn fx:id="missionCostColoumn" prefWidth="123.0" text="Mission Cost" />
        </columns>
      </TableView>
      <ComboBox fx:id="destinationComboBox" layoutX="161.0" layoutY="29.0" onAction="#destinationComboBoxOnAction" prefWidth="150.0" />
      <Label layoutX="34.0" layoutY="31.0" text="Select Destination:">
         <font>
            <Font name="Arial Narrow" size="17.0" />
         </font>
      </Label>
      <BarChart fx:id="barChart" layoutX="56.0" layoutY="386.0" prefHeight="261.0" prefWidth="747.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Mission Cost" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="404.0" layoutY="29.0" mnemonicParsing="false" onAction="#goBackButtonOnClick" text="Go Back" />
   </children>
</AnchorPane>
